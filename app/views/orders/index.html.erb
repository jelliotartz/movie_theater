<div class="app-container">
  <main class="main-content" role="main">
    <header class="header">
      <h1 class="header__heading" id="page-title">
        <%= link_to "tix_flix", movies_path %>
      </h1>
    </header>
    <h1>All Orders</h1>
    <ul>
      <% @orders.each do |order| %>
        <li>
          <%= order.name %>
          <%= order.email %>
        </li>
      <% end %>
    </ul>
  </main>
</div>